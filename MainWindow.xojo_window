#tag Events CalendarView1
	#tag Event
		Sub NewEvent(StartDate As DateTime, EndDate As DateTime)
		  //This code apart from the Listbox line, must be copy pasted in your app.
		  
		  Dim cEvent As CalendarEvent = EditEvent.ShowModal(StartDate, EndDate, me)
		  
		  If cEvent <> Nil then
		    Listbox1.AddRowAt(0, "NewEvent(" + cEvent.Title + " - " + cEvent.StartDate.SQLDate + ")" )
		    me.AddEvent(cEvent)
		    me.Refresh(False)
		  End If
		End Sub
	#tag EndEvent
	#tag Event
		Sub ViewChange(StartDate As DateTime, EndDate As DateTime)
		  Listbox1.AddRowAt(0, "ViewChange(" + StartDate.SQLDate + " - " + EndDate.SQLDate + ")" )
		  
		  If me.ViewType = me.TypeYear then
		    'me.YearMonthsAmount = 3
		  End If
		  
		  //Updating the DatePicker view
		  CalendarView2.DisplayDate = New Date(me.DisplayDate)
		  
		  If me.ViewType = CalendarView.TypeWeek then
		    CalendarView2.SelStart = New Date(StartDate)
		    CalendarView2.SelEnd = New Date(EndDate)
		  else
		    CalendarView2.SelStart = Nil
		  End If
		  CalendarView2.Redisplay()
		  
		  
		End Sub
	#tag EndEvent
	#tag Event
		Function EditEvent(cEvent As CalendarEvent) As CalendarEvent
		  Dim Delete As Boolean
		  'Dim newEvent As CalendarEvent
		  cEvent = EditEvent.ShowModal(cEvent, Delete, me)
		  If Delete then
		    me.RemoveEvent(cEvent, True)
		    Return Nil
		  else
		    Return cEvent
		  End If
		End Function
	#tag EndEvent
	#tag Event
		Sub DragEvent(cEvent As CalendarEvent)
		  If not System.MouseDown then
		    Listbox1.AddRowAt(0, "DragEvent(" + cEvent.Title + " - " + cEvent.StartDate.SQLDateTime + ")" )
		  End If
		End Sub
	#tag EndEvent
	#tag Event
		Function ConstructContextualMenu(base As DesktopMenuItem, x As Integer, y As Integer, cEvent As CalendarEvent) As Boolean
		  If cEvent <> Nil then
		    //Do something here as an event was right-clicked.
		    
		    
		    
		  elseif me.ViewType > me.TypeYear then
		    Dim mn As new MenuItem("New Event")
		    Dim D As Date = me.DateForXY(X, Y)
		    
		    If D <> Nil then
		      If me.ViewType = me.TypeMonth then
		        D.Hour = 0
		        D.Minute = 0
		        D.Second = 0
		      End If
		      
		      mn.Tag = D
		      base.AddMenu mn
		    End If
		  End If
		End Function
	#tag EndEvent
	#tag Event
		Function ShowHelptag(Str As String, cEvent As CalendarEvent) As Boolean
		  //Use this event to display a custom Helptag
		  //Return True to prevent the system helptag from being displayed.
		  
		  Dim txt As String
		  
		  If cEvent <> Nil then
		    txt = str + EndOfLine + cEvent.Description
		  else
		    txt = ""
		  End If
		  
		  If me.Tooltip <> txt then
		    me.Tooltip = txt
		  End If
		  
		  Return True
		End Function
	#tag EndEvent
	#tag Event
		Function DoubleClick(X As Integer, Y As Integer) As Boolean
		  //This enables going from Weekview to Dayview when doubleclicking on a day
		  
		  Dim D As Date = me.DateForXY(X, Y)
		  
		  If D <> Nil then
		    CalendarView1.DisplayDate = D
		    
		    If me.ViewType = me.TypeYear then
		      
		      CalendarView1.ViewType = CalendarView.TypeMonth
		      
		    Else
		      
		      CalendarView1.ViewType = CalendarView.TypeWeek
		      
		    End If
		    
		    Return True
		    
		  Else
		    Dim i As Integer
		    i = 0
		  End If
		End Function
	#tag EndEvent
	#tag Event
		Function CalendarEventFilter(cEvent As CalendarEvent) As Boolean
		  //Return True to this event if the CalendarEvent should be displayed.
		  
		  Dim i As Integer
		  
		  //Here we search for each ColorFilter if its value is True.
		  For i = 0 to 10
		    
		    If cEvent.EventColor = ColorFilter(i).myColor then
		      //We found the corresponding colorFilter
		      //We return its value
		      
		      Return ColorFilter(i).Value
		      
		    End If
		  Next
		  
		  
		  //Return True if the ColorFilter wasn't found
		  Return True
		  
		  
		  //We could also filter usin the cEvent.Tag property that can hold any type of information
		End Function
	#tag EndEvent
