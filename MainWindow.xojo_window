#tag Events PushButton13
	#tag Event
		Sub Pressed()
		  Dim g as Graphics
		  
		  Dim p as PrinterSetup
		  
		  p=New PrinterSetup
		  
		  If p.ShowPageSetupDialog(self) then
		    
		    //The greater the value, the smaller the text will be when printed.
		    p.maxHorizontalResolution=120
		    p.maxVerticalResolution=120
		    
		    g = p.ShowPrinterDialog(self)
		    
		    If g <> Nil then
		      
		      //Prevent the calendarView from refreshing
		      CalendarView1.Freeze = True
		      
		      //Creating a clip of half the size of the paper sheet
		      Dim ge As Graphics = g.Clip(0, 0, g.Width, g.Height\2-10)
		      
		      //Print first month
		      CalendarView1.PrintAdvanced(ge)
		      
		      //Go to next month
		      CalendarView1.Scroll(1)
		      
		      //Creating a clip for the other half of the paper sheet
		      ge = g.clip(0, g.height\2+10, g.Width, g.Height\2-10)
		      
		      //Print second month
		      CalendarView1.PrintAdvanced(ge)
		      
		      //Go back to the original date
		      CalendarView1.Scroll(-1)
		      
		      //Allow CalendarView updates
		      CalendarView1.Freeze = False
		      
		    End if
		    
		    
		  End if
		  
		End Sub
	#tag EndEvent
