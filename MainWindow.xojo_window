#tag Events ImportDBButton
	#tag Event
		Sub Pressed()
		  Dim RS As RowSet
		  
		  //First we make a RecordSet containing all records from the database
		  If EventDB <> Nil and EventDB.Connect then
		    MsgBox("Loading database")
		    RS = EventDB.SelectSQL("SELECT * From Events Where 1")
		    
		    If RS is Nil then
		      Return
		    End If
		    
		    If RS.RowCount = 0 then
		      MsgBox("No Records found")
		      Return
		    End If
		    
		    //Removing all CalendarEvent from the CalendarView
		    CalendarView1.DeleteAllEvents()
		    
		    //Now we send the RecordSet to the CalendarView
		    //All string parameters are optional. They represent the Field name of each event in the database
		    //If the database doesn't contain that many fields, set the field to an empty String.
		    //For example if there is no Location field, you should use:
		    // If CalendarView1.ImportFromDB(RS, "ID", "Start", "End", "Title", "Color", "", "Description") Then
		    
		    If CalendarView1.ImportFromDB(RS, "ID", "Start", "End", "Title", "Color", "Location", "Description", "Recurrence", True, False) Then
		      'MsgBox("Success")
		    Else
		      MsgBox("Fail")
		    End If
		  End If
		End Sub
	#tag EndEvent
#tag EndEvents
