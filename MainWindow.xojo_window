		Sub Pressed()
		  //Exporting events to the database
		  //Only database parameter and table name are mandatory.
		  //All other string parameters are optional. They represent the Field name of each event in the database
		  //If the database doesn't contain that many fields, set the field to an empty String.
		  //For example if there is no Location field, you should use:
		  // If CalendarView1.ExportToDB(database, "Events", "ID", "Start", "End", "Title", "Color", "", "Description") Then
		  
		  
		  
		  If CalendarView1.ExportToDB(EventDB, "Events", "ID", "Start", "End", "Title", "Color", "Location", "Description") then
		    'MsgBox("Success")
		  Else
		    MsgBox("Fail")
		  End If
		End Sub
	#tag EndEvent
