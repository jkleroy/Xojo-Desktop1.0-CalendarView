		Sub Pressed()
		  //Imports iCalendar Data from the Clipboard
		  
		  'Dim mp As new MethodProfiler(CurrentMethodName)
		  
		  If TargetMacOS and Keyboard.CommandKey then
		    CalendarView1.DeleteAllEvents()
		  End If
		  If TargetWin32 and Keyboard.ControlKey then
		    CalendarView1.DeleteAllEvents()
		  End If
		  
		  Dim c As New Clipboard
		  If c.TextAvailable then
		    CalendarView1.ImportICS(c.Text, &c444444, True)
		  End If
		  c.Close
		  
		  
		End Sub
