#tag Events SegmentedControl1
	#tag Event
		Sub Action(itemIndex as integer)
		  Static BaseSize As Integer
		  
		  If BaseSize = 0 then
		    BaseSize = 12
		  End If
		  
		  If itemIndex = 1 then
		    
		    BaseSize = BaseSize + 1
		  Else
		    BaseSize = BaseSize - 1
		  End If
		  
		  CalendarView1.MyStyle.MEventTextSize = BaseSize
		  CalendarView1.MyStyle.MEventHeight = BaseSize +2
		  CalendarView1.MyStyle.WTextSize = BaseSize
		  CalendarView2.MyStyle.PTextSize = BaseSize
		  If CalendarView1.MyStyle.WHeaderHeight>0 then
		    CalendarView1.MyStyle.WHeaderHeight = BaseSize +1
		  End If
		  If CalendarView1.MyStyle.WHeaderTextSize = CalendarView1.MyStyle.WBodyTextSize then
		    CalendarView1.MyStyle.WHeaderTextSize = BaseSize
		    CalendarView1.MyStyle.WBodyTextSize = BaseSize
		  Elseif CalendarView1.MyStyle.WHeaderTextSize < CalendarView1.MyStyle.WBodyTextSize then
		    CalendarView1.MyStyle.WHeaderTextSize = BaseSize-1
		    CalendarView1.MyStyle.WBodyTextSize = BaseSize
		  Else
		    CalendarView1.MyStyle.WHeaderTextSize = BaseSize+1
		    CalendarView1.MyStyle.WBodyTextSize = BaseSize
		  End If
		  CalendarView1.MyStyle.MNumbersTextSize = BaseSize
		  
		  CalendarView1.Redisplay
		  CalendarView2.Redisplay
		  
		End Sub
