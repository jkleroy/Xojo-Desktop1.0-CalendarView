		Function EditOrDeleteRecurrentEvent(cEvent As CalendarEvent) As Boolean
		  #Pragma Unused cEvent
		  
		  Dim n As Integer
		  
		  n = MsgBox("Do you want to edit all events of this recurrence set ?", 36)
		  
		  If n = 6 then
		    
		    Return True
		    
		  Else
		    
		    Return False
		    
		  End If
		End Function
