		  For i as Integer = 0 to 47
		    
		    If i mod 2 = 0 then
		      me.AddRow Str(Floor(i/2), "0#") + ":00"
		    else
		      me.AddRow Str(Floor(i/2), "0#") + ":30"
		    End If
		  Next
		  
