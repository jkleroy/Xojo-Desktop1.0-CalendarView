		Sub Open()
		  If CalendarView.DayNames(1) = "" then Return
		  
		  For i as Integer = 0 to 6
		    
		    me.Items(i).Title = Uppercase(CalendarView.DayNames((i+1) mod 7 + 1).Left(2))
		    
		  Next
		  
		  
		End Sub
